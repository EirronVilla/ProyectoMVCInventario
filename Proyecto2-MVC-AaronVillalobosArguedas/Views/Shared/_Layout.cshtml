<!DOCTYPE html>
<html lang="en">

<head>

	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<title>@ViewData["Title"]</title>

	<!-- Custom fonts for this template-->
	<link href="vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
	<link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
		  rel="stylesheet">

	<!-- Custom styles for this template-->
	<link href="~/css/sb-admin-2.min.css" rel="stylesheet">

</head>

<body id="page-top">

	<!-- Page Wrapper -->
	<div id="wrapper">

		<!-- Sidebar -->
		<ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">

			<!-- Sidebar - Brand -->
			<a class="sidebar-brand d-flex align-items-center justify-content-center" asp-area="" asp-controller="Home" asp-action="Index">
				<div class="sidebar-brand-icon rotate-n-15">
					<i class="fas fa-laugh-wink"></i>
				</div>
				<div class="sidebar-brand-text mx-3">Invent Systems</div>
			</a>

			<!-- Divider -->
			<hr class="sidebar-divider my-0">

			<!-- Nav Item - Dashboard -->
			<li class="nav-item">
				<a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">
					<i class="fas fa-fw fa-tachometer-alt"></i>
					<span>Resumen</span>
				</a>
			</li>

			<!-- Divider -->
			<hr class="sidebar-divider">

			<!-- Heading -->
			<div class="sidebar-heading">
				Inventarios
			</div>

			<!-- Nav Item - Pages Collapse Menu -->
			<li class="nav-item">
				<a class="nav-link" asp-area="" asp-controller="Productos" asp-action="Index">
					<i class="fas fa-fw fa-cog"></i>
					<span>Productos</span>
				</a>
			</li>

			<li class="nav-item">
				<a class="nav-link collapsed" asp-area="" asp-controller="Pedidos" asp-action="Index">
					<i class="fas fa-fw fa-cog"></i>
					<span>Pedidos</span>
				</a>
			</li>

			<li class="nav-item">
				<a class="nav-link collapsed" asp-area="" asp-controller="Retiros" asp-action="Index">
					<i class="fas fa-fw fa-cog"></i>
					<span>Retiros</span>
				</a>
			</li>

@*			<li class="nav-item">
				<a class="nav-link collapsed" asp-area="" asp-controller="Historial" asp-action="Index">
					<i class="fas fa-fw fa-cog"></i>
					<span>Historial</span>
				</a>
			</li>*@


			<!-- Divider -->
			<hr class="sidebar-divider">

			<!-- Heading -->
			<div class="sidebar-heading">
				Sistema
			</div>

			<!-- Nav Item - Pages Collapse Menu -->
			<li class="nav-item">
				<a class="nav-link collapsed" asp-area="" asp-controller="Home" asp-action="Privacy">
					<i class="fas fa-fw fa-cog"></i>
					<span>Sobre Nosotros</span>
				</a>
			</li>



			<!-- Divider -->
			<hr class="sidebar-divider d-none d-md-block">

		</ul>
		<!-- End of Sidebar -->
		<!-- Content Wrapper -->
		<div id="content-wrapper" class="d-flex flex-column">

			<!-- Main Content -->
			<div id="content">

				<!-- Topbar -->
				<nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">
					<!-- Topbar Navbar -->
					<ul class="navbar-nav ml-auto">
						<div class="topbar-divider d-none d-sm-block"></div>

						<!-- Nav Item - User Information -->
						<li class="nav-item dropdown no-arrow">
							<partial name="_LoginPartial" />
						</li>

					</ul>

				</nav>
				<!-- End of Topbar -->
				<!-- Begin Page Content -->
				<div class="container-fluid">

					<!-- Page Heading -->
					@RenderBody()

				</div>
				<!-- /.container-fluid -->

			</div>
			<!-- End of Main Content -->
			<!-- Footer -->
			<footer class="sticky-footer bg-white">
				<div class="container my-auto">
					<div class="copyright text-center my-auto">
						<span>Copyright &copy; Your Website 2020</span>
					</div>
				</div>
			</footer>
			<!-- End of Footer -->

		</div>
		<!-- End of Content Wrapper -->

	</div>
	<!-- End of Page Wrapper -->
	<script src="~/js/vendor/jquery/jquery.min.js"></script>
	<script src="~/js/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
	<script src="~/js/vendor/jquery-easing/jquery.easing.min.js"></script>
	<script src="~/js/sb-admin-2.min.js"></script>
	@await RenderSectionAsync("Scripts", required: false)


</body>

</html>